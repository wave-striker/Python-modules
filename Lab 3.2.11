# Ask the user to enter a word
user_word = input("Enter a word: ")

# Convert the entered word to uppercase
user_word = user_word.upper()

# Initialize an empty string to store the word without vowels
word_without_vowels = ""

# Iterate over each letter in the user's word
for letter in user_word:
    if letter in "AEIOU":  # Check if the letter is a vowel
        continue  # Skip the rest of the loop and move to the next iteration
    # Concatenate the uneaten (non-vowel) letter to the word_without_vowels string
    word_without_vowels += letter

# Print the final word without vowels
print(word_without_vowels)
